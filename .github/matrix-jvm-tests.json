[ {
  "name": "17",
  "java-version": 17,
  "maven_args": "$JVM_TEST_MAVEN_ARGS",
  "maven_opts": "-Xmx2g -XX:MaxMetaspaceSize=1g",
  "os-name": "ubuntu-latest",
  "runs-on": "family=m7i.xlarge/image=ubuntu24-full-x64"
}
, {
  "name": "21",
  "java-version": 21,
  "java-version-gradle": 20,
  "maven_args": "$JVM_TEST_MAVEN_ARGS",
  "maven_opts": "-Xmx3g -XX:MaxMetaspaceSize=1g",
  "os-name": "ubuntu-latest",
  "runs-on": "family=m7i.xlarge/image=ubuntu24-full-x64"
}
, {
  "name": "17 Windows",
  "java-version": 17,
  "maven_args": "-DskipDocs -Dformat.skip",
  "maven_opts": "-Xmx2g -XX:MaxMetaspaceSize=1g",
  "os-name": "windows-latest",
  "runs-on": "family=m7i.xlarge/image=windows22-base-x64"
}]
